{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "prConcurrentLimit": 0,
  "branchConcurrentLimit": 0,
  "prHourlyLimit": 0,
  "pinDigests": true,
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": [".github/workflows/release.yaml"],
      "matchStrings": [
        "tags:\\s*loeken/nzbget:(?<currentValue>v?\\d+\\.\\d+-ls\\d+)"
      ],
      "datasourceTemplate": "docker",
      "packageNameTemplate": "linuxserver/nzbget",
      "versioningTemplate": "loose",
      "extractVersionTemplate": "^v?\\d+\\.\\d+-ls\\d+$"
    }
  ],
  "packageRules": [
    {
      "matchPackageNames": ["linuxserver/nzbget"],
      "registryUrls": ["https://index.docker.io"]
    }
  ]
}
